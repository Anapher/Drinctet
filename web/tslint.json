{
    "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
    "linterOptions": {"exclude": ["node_modules/**/*.ts", "config/**/*.js"]},
    "rules": {
        "trailing-comma": [
            true,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ],
        "eofline": true
    }
}